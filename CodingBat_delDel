public String delDel(String str) {

// Yousef's UPDATED Solution:

   String result = "";
  if(str.length() > 3 && str.substring(1,4).contains("del")){
    result += str.replace("del","");
  }else{
    return str;
  }
  return result; 

// Orkhan's Solution:
  String result = "";
  if (str.length()>3 && str.substring(1, 4).contains("del")) {
    result += str.substring(0,1)+str.substring(4);
  }else{
    return str;
  }
  return result;

}
