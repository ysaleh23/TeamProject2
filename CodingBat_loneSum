
// Orkhan's Solution

public int loneSum(int a, int b, int c) {
int num=0;
if(a==b && b==c)
return 0;
num = (a==b)? c :(b==c)? a :(a==c) ? b :(a+b+c);
return num;

//Yousef's Solution (edited!)

public int loneSum(int a, int b, int c) {
  
  int sum = a+b+c;
  
  
 if(a==b && a == c ){
    sum = 0;
  }else if(a==b){
    sum = c;
  }else if(a==c){
    sum = b;
  }else if(b==c){
    sum = a;
  }
  
  return sum; 

// Hande's Solution

public int loneSum(int a, int b, int c) {
  
  return (a==b && a==c)? 0:(a==b)? c:(a==c)? b:(b==c)? a:a+b+c;
    

}
